_OPER=$1
case ${_OPER} in
	"start" ) 
			echo Starting su - ${IIBADMIN} -c mqsistart ${IIBNODE}
			su - ${IIBADMIN} -c "mqsistart ${IIBNODE}"
	;;
	"stop" )
			echo  Stopping su - ${IIBADMIN} -c mqsistop ${IIBNODE}
			su - ${IIBADMIN} -c "mqsistop ${IIBNODE}"
	;;
esac